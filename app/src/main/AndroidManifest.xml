<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uzb.progressive_young_team.opendataproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".chat.DoctorInfoActivity"
            android:parentActivityName=".chat.DoctorsListActivity"></activity>
        <activity android:name=".chat.DoctorsListActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".authentication.LoginRegisterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".authentication.LoginActivity"
            android:parentActivityName=".authentication.LoginRegisterActivity" />
        <activity
            android:name=".authentication.AuthenticationActivity"
            android:parentActivityName=".authentication.LoginRegisterActivity" />
        <activity
            android:name=".illness_library.IllnessInfoActivity"
            android:parentActivityName=".illness_library.IllnessListActivity" />
        <activity
            android:name=".illness_library.IllnessListActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".authentication.VerifyPhoneActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>